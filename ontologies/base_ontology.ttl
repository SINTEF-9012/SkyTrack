                         ] ;
        rdfs:subClassOf  [ rdf:type           owl:Restriction ;
                           owl:allValuesFrom  [ rdf:type     owl:Class ;
                                                owl:unionOf  ( pizza:AsparagusTopping pizza:HamTopping pizza:MozzarellaTopping pizza:ParmesanTopping pizza:TomatoTopping )
